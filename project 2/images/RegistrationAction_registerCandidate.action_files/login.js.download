function HideAll()
{
	for(i = 0; i<=11; i++)
	{
		try
		{
			boxID = document.getElementById("block"+i);
			boxID.style.display = "none";
			if(i == 1)
			{
				document.register.reset();
			}
			if(i == 2)
			{
				document.forgorpwd.reset();
			}
		}
		catch(e)
		{}
	}
}

